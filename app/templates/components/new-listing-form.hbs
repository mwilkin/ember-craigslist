<form>
  <div class="form-group">
    <label for="name">Listing Name</label>
     {{input value=name id="name"}}
  </div>
  <div class="form-group">
    <label for="poster">Poster Name</label>
     {{input value=poster id="poster"}}
  </div>
  <div class="form-group">
    <label for="imageUrl">Image URL</label>
     {{input value=imageUrl id="imageUrl"}}
  </div>
  <div class="form-group">
    <label for="price">Price</label>
     {{input value=price id="price"}}
  </div>
  <div class="form-group">
    <label for="date">Date Listed</label>
     {{input value=date id="date"}}
  </div>
  <button {{action 'saveListing'}}>Save</button>
</form>
